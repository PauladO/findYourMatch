<h1>Users</h1>
<table style="width:80%">
  <tr>
    <th>Name</th>
    <th>email address</th>
    <th>Admin</th>
    <th>User ID</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.admin ? "Admin" : "Student" %></td>
      <td><%= user.id %></td>
    </tr>
  <% end %>
</table>
